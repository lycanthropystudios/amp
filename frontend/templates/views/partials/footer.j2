{{ footer() }}
{% else %}
{# The icons need to be added outside the cached partial #}
{% do doc.icons.useIcon('icons/twitter.svg') %}
{% do doc.icons.useIcon('icons/youtube.svg') %}
{% do doc.icons.useIcon('icons/wordpress.svg') %}
{% do doc.icons.useIcon('icons/github.svg') %}
{% do doc.icons.useIcon('icons/stackoverflow.svg') %}

{{ doc.pod.amp_dev.template_cache.get(cache_key)|safe }}
{% endif %}
